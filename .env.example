# Exemplo de arquivo .env para o projeto FaleComJesus
# Copie este arquivo para .env e configure as variáveis de acordo com seu ambiente

# API URL para o frontend
API_URL=http://localhost:8000

# Credenciais de bancos de dados
AUTH_DB_PASSWORD=changeme
STUDY_DB_PASSWORD=changeme
CHAT_DB_PASSWORD=changeme
BIBLE_DB_PASSWORD=changeme
GAMIFICATION_DB_PASSWORD=changeme
ADMIN_DB_PASSWORD=changeme

# Segurança
JWT_SECRET_KEY=your-jwt-secret-key
ADMIN_TOKEN=your-admin-token

# RabbitMQ
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Elastic Stack
ELASTIC_PASSWORD=changeme

# Ambiente (development, test, production)
NODE_ENV=development 